        <Button x:Name="button7" Content="/" HorizontalAlignment="Left" Margin="295,220,0,0" VerticalAlignment="Top" Height="48" Width="61" FontSize="20" RenderTransformOrigin="-1.565,0.647"/>
        <Button x:Name="button8" Content="4" HorizontalAlignment="Left" Margin="42,288,0,0" VerticalAlignment="Top" Height="52" Width="53" FontSize="20"/>
        <Button x:Name="button9" Content="5" HorizontalAlignment="Left" Margin="120,288,0,0" VerticalAlignment="Top" Height="52" Width="54" FontSize="20"/>
        <Button x:Name="button10" Content="6" HorizontalAlignment="Left" Margin="206,288,0,0" VerticalAlignment="Top" Height="52" Width="59" FontSize="20"/>
        <Button x:Name="button11" Content="*" HorizontalAlignment="Left" Margin="300,288,0,0" VerticalAlignment="Top" Height="52" Width="56" FontSize="22"/>
        <Button x:Name="button_6" Content="1" HorizontalAlignment="Left" Margin="42,368,0,0" VerticalAlignment="Top" Height="50" Width="53" FontSize="20"/>
        <Button x:Name="button_7" Content="2" HorizontalAlignment="Left" Margin="120,368,0,0" VerticalAlignment="Top" Height="50" Width="54" FontSize="20"/>
        <Button x:Name="button14" Content="3" HorizontalAlignment="Left" Margin="206,368,0,0" VerticalAlignment="Top" Height="50" Width="59" FontSize="20"/>
        <Button x:Name="button15" Content="-" HorizontalAlignment="Left" Margin="300,368,0,0" VerticalAlignment="Top" Height="50" Width="56" FontSize="24"/>
        <Button x:Name="button_0" Content="0" HorizontalAlignment="Left" Margin="42,452,0,0" VerticalAlignment="Top" Height="53" Width="53" FontSize="22"/>
        <Button x:Name="button__equal" Content="=" HorizontalAlignment="Left" Margin="120,452,0,0" VerticalAlignment="Top" Height="53" Width="145" FontSize="48" Click="button__equal_Click"/>
        <Button x:Name="button__plus" Content="+" HorizontalAlignment="Left" Margin="300,452,0,0" VerticalAlignment="Top" Height="53" Width="56" FontSize="36"/>
        <Button x:Name="button19" Content="Wpf calculater" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="44" Width="396" FontSize="36"/>

    </Grid>
